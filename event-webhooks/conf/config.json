{
    "regex_webhooks": [
        {
            "name" : "SensorOn",
            "sensor_name_match" : {
                "presenceSensor" : "D4146053-308A-47F5-809A-4244AA86F6B9"
            },
            "sensor_content_match" : {
                "updateCount" : 1
            },
            "hooks" : [ {
                    "url" : "http://ntcip-relay-server:8080/pedcall/",
                    "json" : {
                        "phase_control_group" : 1,
                        "phase" : 1,
                        "activate" : true,
                        "mib" : "1.3.6.1.4.1.1206.4.2.1.1.5.1.7"
                    }
                }
            ]
        },
        {
            "name" : "SensorOff",
            "sensor_name_match" : {
                "presenceSensor" : "D4146053-308A-47F5-809A-4244AA86F6B9"
            },
            "sensor_content_match" : {
                "endedAt" : ".*"
            },
            "hooks" : [ {
                    "url" : "http://ntcip-relay-server:8080/pedcall/",
                    "json" : {
                        "phase_control_group" : 1,
                        "phase" : 1,
                        "activate" : false,
                        "mib" : "1.3.6.1.4.1.1206.4.2.1.1.5.1.7"
                    }
                }
            ]
        }
    ]
}